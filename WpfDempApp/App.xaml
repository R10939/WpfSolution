<Application x:Class="WpfDempApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WpfDempApp"
             StartupUri="Bindings\EligibilityPersonWindow.xaml">
    <Application.Resources>
        
        <ResourceDictionary>
            
            <LinearGradientBrush x:Key= "ALinearBrush1" StartPoint="0,0" EndPoint="1,1">
                <GradientStop Color="Blue" Offset="0"/>
                <GradientStop Color="Cyan" Offset="0.25"/>
                <GradientStop Color="Crimson" Offset="0.50"/>
                <GradientStop Color="Green" Offset="0.75"/>
                <GradientStop Color="Yellow" Offset="1"/>
            </LinearGradientBrush>

            <LinearGradientBrush x:Key= "ALinearBrush2" StartPoint=".5,0" EndPoint=".5,1">
                <GradientStop Color="Orange" Offset="0"/>
                <GradientStop Color="White" Offset="0.5"/>
                <GradientStop Color="Green" Offset="1"/>
            </LinearGradientBrush>

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Asset/DemoResourceDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            
        </ResourceDictionary> 
        
    </Application.Resources>
</Application>
